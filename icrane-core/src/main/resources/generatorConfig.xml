<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >

<generatorConfiguration>
    <classPathEntry location="/src/main/resources/lib/mysql-connector-java-5.1.6.jar"/>
    //驱动包存放的位子
    <context id="context1">
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://127.0.0.1:3306/laoao"
                        userId="root" password="123456"/>
        <javaModelGenerator targetPackage="com.bfei.icrane.core.models" targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resource">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>
        <table schema="" tableName="t_agent" domainObjectName="t_agent"
               enableCountByExample="true"
               enableUpdateByExample="true"
               enableDeleteByExample="true"
               enableSelectByExample="true"
               selectByExampleQueryId="true">
        </table>
    </context>
</generatorConfiguration>